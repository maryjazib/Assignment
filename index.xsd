<?xml version="1.0" encoding="utf-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="HN-stationary">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" name="Products">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Product-id" type="xs:unsignedByte" />
              <xs:element name="Product-name" type="xs:string" />
              <xs:element name="Quantity" type="xs:string" />
              <xs:element name="Price-per-unit" type="xs:unsignedByte" />
              <xs:element name="Price-per-dozen" type="xs:unsignedShort" />
              <xs:element name="stock" type="xs:string" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>